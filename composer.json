{
    "name": "cline/intl",
    "autoload": {
        "psr-4": {
            "Cline\\Intl\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Brian Faust",
            "email": "brian@cline.sh"
        }
    ],
    "require-dev": {
        "cline/php-cs-fixer": "^1.0",
        "pestphp/pest": "^4.1",
        "ergebnis/composer-normalize": "^2.48",
        "orchestra/testbench": "^10.6"
    },
    "require": {
        "commerceguys/addressing": "^2.2",
        "illuminate/contracts": "^12.28",
        "symfony/intl": "^7.3",
        "spatie/laravel-data": "^4.17",
        "brick/postcode": "^0.5.0",
        "brick/phonenumber": "^0.8.0"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "ergebnis/composer-normalize": true
        }
    }
}
